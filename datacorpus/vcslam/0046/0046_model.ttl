@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.

vcslam:Bicycle
    vcslam:refers_to vcslam:Theft;
    vcslam:colour xsd:string ;
    vcslam:type xsd:string ;
    vcslam:includes vcslam:Speed;
    vcslam:created_by vcslam:Organization;
    vcslam:includes vcslam:Cost.

vcslam:Organization 
    vcslam:name xsd:string.

vcslam:Cost
    vcslam:value xsd:anySimpleType.

vcslam:Theft 
  vcslam:includes vcslam:Event.

vcslam:Speed
  vcslam:value xsd:anySimpleType.

vcslam:Event
   vcslam:is_part_of vcslam:Occurence.

 vcslam:Occurence
   vcslam:date xsd:date;
   vcslam:id xsd:string;
   vcslam:year xsd:gYear;
   vcslam:month xsd:gMonth;
   vcslam:day xsd:gDay;
   vcslam:time xsd:time;
   vcslam:locatedAt vcslam:Location;
   vcslam:includes vcslam:Police;
   vcslam:includes vcslam:Theft;
   vcslam:includes vcslam:Offense.
                     
vcslam:Theft 
  vcslam:status xsd:string.

vcslam:Police
  vcslam:includes vcslam:Division.

vcslam:Division 
  vcslam:number xsd:int.
                     
                     
vcslam:Offence 
   vcslam:primary xsd:string.


vcslam:Location
    vcslam:includes vcslam:Neighbourhood;
    vcslam:positionAt vcslam:Position .

vcslam:Neighbourhood 
    vcslam:id xsd:string;
    vcslam:name xsd:string.

vcslam:Position
    vcslam:latitude xsd:decimal ;
    vcslam:geometry_type xsd:string;
    vcslam:longitude xsd:decimal .
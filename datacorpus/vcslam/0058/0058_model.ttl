@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vcslam:Record vcslam:of vcslam:Drinking_fountain ;
    vcslam:id xsd:string ;
    vcslam:timestamp xsd:dateTimeStamp.

vcslam:Drinking_fountain vcslam:name xsd:string;
    vcslam:maintained_by vcslam:Group;
    vcslam:id xsd:string;
    vcslam:locatedAt vcslam:Location;
    vcslam:photo vcslam:Photo;
    vcslam:times_of_operation xsd:string;
    vcslam:pet_friendly xsd:boolean.

vcslam:Group vcslam:name xsd:string.

vcslam:Photo vcslam:name xsd:string.

vcslam:Location vcslam:locatedAt vcslam:Planning_area;
    vcslam:positionAt vcslam:Position.

vcslam:Planning_area vcslam:name xsd:string.

vcslam:Position vcslam:geometry_type xsd:string;
    vcslam:coordinates xsd:decimal.
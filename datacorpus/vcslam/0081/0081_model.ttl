@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.


vcslam:Record vcslam:of vcslam:Tree ;
    vcslam:id xsd:string ;
    vcslam:timestamp xsd:dateTimeStamp.


vcslam:Tree vcslam:is_memorial xsd:boolean;
            vcslam:common_name xsd:string;
            vcslam:scientific_name xsd:string;
            vcslam:consists_of vcslam:Stem;
            vcslam:locatedAt vcslam:Location;
            vcslam:ownership xsd:string;
            vcslam:includes vcslam:Installation;
            vcslam:consists_of vcslam:Trunk.

vcslam:Installation vcslam:date xsd:date.

vcslam:Stem vcslam:count xsd:int.

vcslam:Trunk vcslam:measurement vcslam:Diameter.

vcslam:Diameter vcslam:measurement_unit_distance vcslam:Centimeter;
                vcslam:value xsd:string.

vcslam:Location vcslam:locatedAt vcslam:Park;
                vcslam:positionAt vcslam:Position.

vcslam:Park vcslam:name xsd:string.

vcslam:Position vcslam:geometry_type xsd:string;
    vcslam:coordinates xsd:decimal.
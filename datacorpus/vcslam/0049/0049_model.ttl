@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.

vcslam:Business
    vcslam:refers_to vcslam:License;
    vcslam:name xsd:string;
    vcslam:type xsd:string;
    vcslam:includes vcslam:Employees;
    vcslam:includes vcslam:Trade;              
    vcslam:includes vcslam:Building.

vcslam:Trade
   vcslam:name xsd:string.

vcslam:License 
    vcslam:includes vcslam:Expire;
    vcslam:includes vcslam:Revision;
    vcslam:number xsd:int;
    vcslam:id xsd:string;
    vcslam:status xsd:string ;
    vcslam:includes vcslam:Folder;
    vcslam:date_issued xsd:date;
    vcslam:includes vcslam:Fee.

vcslam:Employees 
   vcslam:number xsd:int.

vcslam:Fee 
    vcslam:value xsd:anySimpleType.

vcslam:Expire
   vcslam:date xsd:date.

vcslam:Revision 
   vcslam:number xsd:int.

vcslam:Folder 
   vcslam:year xsd:gYear.


vcslam:Building
    vcslam:locatedAt vcslam:Location;
    vcslam:includes vcslam:Building_unit.

vcslam:Building_unit 
    vcslam:number xsd:int;
    vcslam:type xsd:string.

vcslam:Location
    vcslam:includes vcslam:Street;
    vcslam:includes vcslam:City;
    vcslam:includes vcslam:Postal;
    vcslam:includes vcslam:Country;
    vcslam:includes vcslam:Vernacular_area;
    vcslam:positionAt vcslam:Position .

vcslam:Street 
    vcslam:name xsd:string.

vcslam:Vernacular_area 
    vcslam:name xsd:string.    

vcslam:Postal 
   vcslam:code xsd:string. 

vcslam:Country 
   vcslam:information xsd:string.

vcslam:City 
    vcslam:name xsd:string.

vcslam:Position
    vcslam:latitude xsd:decimal ;
    vcslam:longitude xsd:decimal .
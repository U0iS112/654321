@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vcslam:Record vcslam:of vcslam:Incident ;
    vcslam:id xsd:string ;
    vcslam:timestamp xsd:dateTimeStamp.

vcslam:Incident vcslam:of vcslam:Graffiti;
    vcslam:count xsd:int;
    vcslam:locatedAt vcslam:Location.

vcslam:Location vcslam:locatedAt vcslam:Planning_area;
    vcslam:positionAt vcslam:Position.


vcslam:Planning_area vcslam:name xsd:string.

vcslam:Position vcslam:coordinates xsd:decimal.
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vcslam:Record vcslam:of vcslam:Traffic;
    vcslam:start_date_time xsd:dateTime;
              vcslam:includes vcslam:Duration.

vcslam:Duration vcslam:measurement_unit_time vcslam:Day;
                vcslam:value xsd:string  .

vcslam:Traffic vcslam:locatedAt vcslam:Location.

vcslam:Location vcslam:locatedAt vcslam:Intersection;
    vcslam:description xsd:string;
    vcslam:positionAt vcslam:Position.

vcslam:Position vcslam:latitude xsd:decimal;
    vcslam:longitude xsd:decimal.
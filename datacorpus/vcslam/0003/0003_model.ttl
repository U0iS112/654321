@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.

vcslam:Crash 
    vcslam:id xsd:string;
    vcslam:condition  vcslam:Condition;
    vcslam:date xsd:date ;
    vcslam:time xsd:time;
    vcslam:locatedAt vcslam:Location;
    vcslam:severity xsd:string;
    vcslam:compass_direction xsd:string;
    vcslam:at_intersection xsd:boolean;
    vcslam:at_midblock xsd:boolean.
    
vcslam:Condition 
    vcslam:road vcslam:Road;
    vcslam:visibility xsd:string;
    vcslam:weather  vcslam:Weather.

vcslam:Weather
    vcslam:information xsd:string.


vcslam:Road
    vcslam:information xsd:string.


vcslam:Location 
    vcslam:positionAt vcslam:Position;
    vcslam:includes vcslam:Suburb.

vcslam:Suburb
    vcslam:name xsd:string.  

vcslam:Position 
    vcslam:latitude xsd:decimal ;
    vcslam:longitude xsd:decimal .



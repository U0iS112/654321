@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.

vcslam:Summon vcslam:for vcslam:Violation;
    vcslam:id xsd:string;
    vcslam:date_time xsd:dateTime;
    vcslam:includes vcslam:Fine.

vcslam:Fine vcslam:amount xsd:string .

vcslam:Violation vcslam:time xsd:time;
    vcslam:description xsd:string;
    vcslam:locatedAt vcslam:Location.

vcslam:Location vcslam:locatedAt vcslam:Street;
    vcslam:locatedAt vcslam:Council_district;
    vcslam:locatedAt vcslam:Police_district;
    vcslam:locatedAt vcslam:Census_tract;
    vcslam:locatedAt vcslam:Census_Block;
    vcslam:human_address xsd:string;
    vcslam:locatedAt vcslam:City;
    vcslam:locatedAt vcslam:State;
    vcslam:positionAt vcslam:Position.

vcslam:Street vcslam:name xsd:string;
    vcslam:number xsd:int;
    vcslam:address xsd:string.

vcslam:City vcslam:name xsd:string.

vcslam:State vcslam:name xsd:string.

vcslam:Police_district vcslam:identifier xsd:string.

vcslam:Council_district vcslam:identifier xsd:string.

vcslam:Census_tract vcslam:identifier xsd:string.

vcslam:Census_Block vcslam:includes vcslam:Group;
                    vcslam:identifier xsd:string .

vcslam:Group vcslam:identifier xsd:string.

vcslam:Position vcslam:longitude xsd:decimal;
    vcslam:latitude xsd:decimal.


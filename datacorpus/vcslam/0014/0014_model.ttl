@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vcslam:Fire_incident
    vcslam:number xsd:int ;
    vcslam:date_time  xsd:dateTime ;
    vcslam:locatedAt vcslam:Location;
    vcslam:includes vcslam:Response .
    
vcslam:Response
    vcslam:type xsd:string.
  

vcslam:Location
    vcslam:address xsd:string ;   
    vcslam:positionAt vcslam:Position .

vcslam:Position
    vcslam:latitude xsd:decimal ;
    vcslam:geometry_type xsd:string;
    vcslam:longitude xsd:decimal .    
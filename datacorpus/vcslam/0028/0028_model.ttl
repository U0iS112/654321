@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vcslam:Business
    vcslam:refers_to vcslam:License;
    vcslam:includes vcslam:Category;
    vcslam:includes vcslam:Trade;  
    vcslam:locatedAt vcslam:Location .

vcslam:Category
    vcslam:description xsd:string.

vcslam:Trade
    vcslam:name xsd:string.

vcslam:Location
    vcslam:includes vcslam:Neighbourhood;
    vcslam:includes vcslam:Ward;
    vcslam:address xsd:string ;
    vcslam:positionAt vcslam:Position .

vcslam:Neighbourhood
    vcslam:id xsd:string;
    vcslam:name xsd:string.

vcslam:Ward
    vcslam:information xsd:string;

vcslam:Position
    vcslam:latitude xsd:decimal ;
    vcslam:longitude xsd:decimal .


vcslam:Permit 
    vcslam:status xsd:string ;
    vcslam:date_issued xsd:date ;
    vcslam:includes vcslam:Expire.

vcslam:Expire 
    vcslam:date xsd:date.    
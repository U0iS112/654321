@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vcslam:Parking
    vcslam:refers_to vcslam:Occupancy;
    vcslam:includes vcslam:Time_period;
    vcslam:includes vcslam:Space;
    vcslam:includes vcslam:Category;
    vcslam:locatedAt vcslam:Location .

vcslam:Category
    vcslam:description xsd:string.

vcslam:Time_period
    vcslam:number xsd:int;
    vcslam:measurement_unit_time vcslam:Minute.    

vcslam:Space
    vcslam:count xsd:int.

vcslam:Occupancy
    vcslam:paid vcslam:Payment;
    vcslam:date_time xsd:dateTime.

vcslam:Payment
    vcslam:number xsd:number.    

vcslam:Block
    vcslam:name xsd:string.

vcslam:Street
    vcslam:side_of xsd:string.

vcslam:Location
    vcslam:includes vcslam:Block;
    vcslam:includes vcslam:Street;
    vcslam:positionAt vcslam:Position .

vcslam:Position
    vcslam:latitude xsd:decimal ;
    vcslam:longitude xsd:decimal .

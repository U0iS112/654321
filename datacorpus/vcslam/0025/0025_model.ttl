@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vcslam:Traffic
    vcslam:involves vcslam:Disruption.     
    

vcslam:District 
    vcslam:information xsd:string.

vcslam:Disruption
    vcslam:id xsd:string ;
    vcslam:includes vcslam:Report;
    vcslam:status xsd:string ;
    vcslam:impact xsd:string ;
    vcslam:description xsd:string ;
    vcslam:includes vcslam:Duration;
    vcslam:includes vcslam:Activity;
    vcslam:locatedAt vcslam:Location .

vcslam:Duration
    vcslam:description xsd:string.    

vcslam:Activity
    vcslam:type xsd:string.  
    vcslam:affects vcslam:Infrastructure.

vcslam:Infrastructure
    vcslam:description.    

vcslam:Report
    vcslam:date_issued xsd:date ;
    vcslam:includes vcslam:Time_period. 

vcslam:Time_period
    vcslam:includes vcslam:Start;
    vcslam:includes vcslam:End.    

vcslam:Start
    vcslam:date xsd:date.

vcslam:End
    vcslam:date xsd:date.

vcslam:Location
    vcslam:includes vcslam:Street; 
    vcslam:includes vcslam:District;
    vcslam:positionAt vcslam:Position .

vcslam:Street 
    vcslam:from xsd:string;
    vcslam:on xsd:string;
    vcslam:to xsd:string,

vcslam:Position
    vcslam:latitude xsd:decimal ;
    vcslam:longitude xsd:decimal .

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vcslam:Washroom
    vcslam:maintained_by vcslam:Organization;
    vcslam:name xsd:string;
    vcslam:wheelchair_accessible xsd:boolean;
    vcslam:id xsd:string;
    vcslam:note xsd:string;
    vcslam:type xsd:string;
    vcslam:locatedAt vcslam:Location.

vcslam:Record vcslam:id xsd:string;
    vcslam:of vcslam:Washroom.

vcslam:Location
    vcslam:positionAt vcslam:Position;
    vcslam:locatedAt vcslam:Planning_area;
    vcslam:information xsd:string;
    vcslam:address xsd:string.

vcslam:Position vcslam:coordinates xsd:decimal;
    vcslam:geometry_type xsd:string.

vcslam:Planning_area vcslam:name xsd:string.


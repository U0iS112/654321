@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vcslam: <http://www.vcslam.tmdt.info/schema#>.

vcslam:Owner vcslam:information "properties.Permit_Holder".

vcslam:Street vcslam:number "properties.Permit_Address_Street_No".

vcslam:Street vcslam:name "properties.Permit_Address_Street_Name".

vcslam:Postal vcslam:code "properties.Permit_Address_Postal_Code".

vcslam:Ward vcslam:identifier "properties.Permit_Address_Ward".

vcslam:Permit vcslam:contact_information "properties.Permit_Contact_Phone_No_".

vcslam:Coordinates vcslam:geometry_type "geometry.type".

vcslam:Coordinate_pair vcslam:latitude "geometry.coordinate.latitude".

vcslam:Coordinate_pair vcslam:longitude "geometry.coordinate.longitude".

vcslam:Permit vcslam:type "properties.Permit_Class".

vcslam:Permit vcslam:id "properties.Permit_No".